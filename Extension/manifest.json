{
  "manifest_version": 3,
  "name": "MediaCred Extension",
  "description": "Credibility evaluator for various types of content",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": 
    [
        "tabs",
        "tabGroups"
    ]
}